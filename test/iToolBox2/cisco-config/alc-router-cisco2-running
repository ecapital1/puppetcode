 policy group IT-Policy
   functions file-access
   functions file-browse
   functions file-entry
   functions svc-enabled
   filter tunnel ALC-VPN-IT
   svc address-pool "VPN-ALC-IT-POOL" netmask 255.255.255.192
   svc default-domain "epochcapital.com.au"
   svc keep-client-installed
   svc split include 10.10.10.0 255.255.255.0
   svc split include 10.129.1.0 255.255.255.0
   svc split include 10.10.24.0 255.255.255.128
   svc dns-server primary 10.10.10.2
   svc dns-server secondary 10.129.1.21
   no svc dtls
   url-list "EpochSites"
   cifs-url-list "FileShares"
