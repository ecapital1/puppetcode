 action 1.2 cli command "show mem stat | append flash:memory.txt"
 action 1.3 cli command "show mem alloc total | append flash:memory.txt"
 action 1.4 cli command "show proc mem sort | append flash:memory.txt"
 action 1.5 cli command "show buffers | append flash:memory.txt"
 action 1.6 cli command "show buffers old | append flash:memory.txt"
 action 1.7 cli command "show buffers assigned | append flash:memory.txt"
 action 1.8 cli command "show buffers assigned packet | append flash:memory.txt"
 action 1.9 cli command "show memory dead | append flash:memory.txt"
 action 2.0 cli command "show log | append flash:memory.txt"
 action 2.1 cli command "show proc cpu | append flash:memory.txt"
 action 2.2 cli command "show mem dead totals | append flash:memory.txt"
 action 2.3 cli command "show mem sum | append flash:memory.txt"
 action 2.4 cli command "show tech | append flash:memory.txt"
 action 3.0 syslog msg "Memory information collected"
 action 3.1 cli command "end"
!
