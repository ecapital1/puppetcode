 http-redirect port 80
 ssl trustpoint epochcapital.com.au
 inservice
 !
webvpn context EPCAU-VPN
 title "Epoch Capital"
 login-photo file EPOCH-Icon-Dark-RGB.png
 logo file epoch.padded.png
 color #4b5c65
 secondary-color #4b5c65
 login-message "Epoch Capital Unauthorized Access Prohibited"
 aaa authentication list ssl_ent
 gateway gateway_1
 max-users 50
 !
 cifs-url-list "FileShares"
   heading "File Shares"
   url-text "Sydney File Share" url-value "10.10.10.7"
 !
 ssl authenticate verify all
 !
 url-list "EpochSites"
   heading "Epoch Sites"
   url-text "Confluence" url-value "confluence.epochcapital.com.au"
 inservice
 !
 policy group IT-Policy
   functions file-access
   functions file-browse
   functions file-entry
   functions svc-enabled
   filter tunnel EPCAU-VPN-IT
   svc address-pool "VPN-EPCAU-IT-POOL" netmask 255.255.255.192
   svc default-domain "epochcapital.com.au"
   svc keep-client-installed
   svc split include 10.10.10.0 255.255.255.0
   svc split include 10.129.1.0 255.255.255.0
   svc split include 10.10.24.0 255.255.255.128
   svc split include 10.10.20.0 255.255.255.0
   svc dns-server primary 10.10.10.2
   svc dns-server secondary 10.129.1.21
   no svc dtls
   url-list "EpochSites"
   cifs-url-list "FileShares"
 !
